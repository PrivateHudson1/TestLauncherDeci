<Application x:Class="TestLauncher.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:TestLauncher"
             StartupUri="Views/Windows/MainWindow.xaml">
    
    
    
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Views/Styles/ApplicationStyles.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <FontFamily x:Key="MinecraftRus">pack://application:,,,/Views/Fonts/#Minecraft Rus</FontFamily>
        </ResourceDictionary>
    </Application.Resources>
</Application>
